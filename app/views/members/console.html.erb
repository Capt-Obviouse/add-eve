<div class="row">
	<div class='col-12'>
		<div class='content-background'>
			<div class="row">
				<div class="col-12">
					<div class='title'>
						Console
					</div>
				</div>
			</div>
			<div class="row">
				<div class='col-2'>
					<h3>Characters:</h3>
				</div>
				<div class='col-10'>
					<% @characterArray.each do |a| %>
					<%= a %> <br>
					<% end %>
				</div>
			</div>
		</div>
	</div>
</div>
<div class="row">
	<div class='col-12'>
		<%= render 'sidebars/console_cluster' %>
	</div>
</div>
<div class='row'>
	<div class='col-12'>
		<div class='content-background'>
			<div class='row'>
				<div class='col-12'>
					<div class='title'>
					News
					</div>
				</div>
			</div>
			<div class='row'>
				<div class='col-12'>
						<%= render 'news/latest_posts' %>
				</div>
			</div>
		</div>
	</div>
</div>
